{
  "description": "Find-a-Way Solver",
  "engines": {
    "node": "15.5.0"
  },
  "scripts": {
    "clean": "rm -rf dist node_modules",
    "node:version": "v=`node -v`; w=qv$npm_package_engines_node; [ $v = $w ] || echo Node $v should be $w",
    "deps": "node -v; npm install --no-save",
    "test": "nodemon --watch ./src --ext js,jsx --exec npm run test:once",
    "test:once": "clear; eslint --fix . && jest --bail",
    "test:once:dumb": "TERM=dumb npm run test:once",
    "build:dev": "webpack --mode development",
    "build": "webpack",
    "start": "npm run test:once; webpack serve"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "styled-components"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.3.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.9.0"
  },
  "dependencies": {
    "http-server": "^14.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "none"
  },
  "license": "UNLICENSED",
  "name": "find-a-way",
  "version": "1.0.0",
  "author": "Denis Howe"
}
