{
  "description": "Find-a-Way Solver",
  "scripts": {
    "install": "npm install --global eslint eslint-config-airbnb eslint-plugin-import eslint-plugin-jest eslint-plugin-react jest nodemon webpack webpack-cli webpack-dev-server; npm install --save-dev @babel/core @babel/preset-env @babel/preset-react",
    "test": "nodemon --watch ./src --ext js,jsx --exec npm run test:once",
    "test:once": "clear; eslint --fix . && jest --bail",
    "test:once:dumb": "TERM=dumb npm run test:once",
    "build": "webpack --mode production",
    "start": "npm run test:once; webpack-dev-server --mode development"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "styled-components"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.0.5",
    "eslint": "^7.7.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "http-server": "^0.12.3"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.3.2",
    "react-dom": "^16.7.0",
    "styled-components": "^5.1.1"
  },
  "repository": {
    "type": "git",
    "url": "none"
  },
  "license": "UNLICENSED",
  "name": "find-a-way",
  "version": "1.0.0",
  "author": "Denis Howe"
}
